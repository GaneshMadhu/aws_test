var has_data = "<%= @post_metrics && @post_metrics['data'] && @post_metrics['data']['avg_time'].present? %>";
if(has_data == 'true'){
  $("#post_metrics_chart_wrapper").html("<%= escape_javascript(render partial: 'charts', locals: { post_metrics: @post_metrics["data"] }) %>");
  ReactRailsUJS.mountComponents('#zoom_in_react_wrapper');
}
else{
  	$("#post_metrics_chart_wrapper").html("<%= escape_javascript(render partial: 'shared/no_results' ) %>");
}
$("#dynamic_reset_filters").html("<%= escape_javascript(render partial: 'shared/reset_filters', locals: { data: (@post_metrics['filters'] rescue nil), reset: (@post_metrics['reset_options'] rescue nil) }) %>").html('');
set_trait_details();
remove_loaders();

