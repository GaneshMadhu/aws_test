var has_data = "<%= @post_metrics && @post_metrics['data'] && @post_metrics['data']['avg_time'].present? %>";
if(has_data == 'true'){
  $("#post_metrics_chart_wrapper").html("<%= escape_javascript(render partial: 'charts', locals: { post_metrics: @post_metrics["data"] }) %>");
  ReactRailsUJS.mountComponents();
}
else{
  $("#post_metrics_chart_wrapper").html("<%= escape_javascript(render partial: 'shared/no_results' ) %>");
}
remove_loaders();
