<script>
  <% if data && reset %>
    <% reset['reset_filters'].each do |type| %>
      <% next if data.blank? or data[type].blank? or data[type].compact.blank? %>
      var wrapper = "#<%= type %>_filter_wrapper";
      $(wrapper).html("<%= escape_javascript(react_component('FilterSelect', SelectFilterParams.new(data[type], type, filter_case_study_library_index_path, (reset['selected'] || {})).options)) %>");
      ReactRailsUJS.mountComponents(wrapper);
    <% end %>
  <% end %>
</script>
